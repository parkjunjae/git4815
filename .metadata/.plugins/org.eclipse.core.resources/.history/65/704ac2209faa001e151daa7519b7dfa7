package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a , b; 
		a = sc.nextInt();
		b = sc.nextInt();
		if(a > 0 && b<10) {
			System.out.println(a+b);
		}
		else {
			System.out.println("실행이 잘못 됐습니다.");
		}
		
		
	}
	

}
