package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int h = 0, m = 0;
		h = sc.nextInt();
		m = sc.nextInt();
		
		if(m>=45) {
			System.out.println(h+" "+ m);
		}
		else if(m<45) {
			if(h==0) {
				h = 23;
				m += 15;
				System.out.println(h+" "+ m);
			}
		
		else if(h > 0) {
			h -= 1;
			m += 15;
			System.out.println(h+" "+ m);
			}
		}
		sc.close();
	}
	
}
