package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		
		
		if(a==2) {
		for(int i = 1; i<10; i++) {
			System.out.println( a +" *"+ i + " =" + (a * i ));
			}
		}
		if(a==2) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==3) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==4) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==5) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==6) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==7) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else if(a==8) {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
			}
		else {
			for(int i = 1; i<10; i++) {
				System.out.println( a +" *"+ i + " =" + (a * i ));
				}
		}
		
			sc.close();
	}
	

}
