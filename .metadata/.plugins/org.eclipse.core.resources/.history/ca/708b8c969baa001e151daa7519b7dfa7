package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, n;
		a = sc.nextInt();
		n = sc.nextInt();
		
		if(a==2) {
		for(n = 1; n<10; n++) {
			System.out.println( a +" *"+ n + " =" + (a * n ));
			}
		}
		
			sc.close();
	}
	

}
