package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, n;
		a = sc.nextInt();
		n = sc.nextInt();
		
		if(a==2) {
		for(int i = 1; i<10; i++) {
			System.out.println( a +" *"+ i + " =" + (a * i ));
			}
		}
		
			sc.close();
	}
	

}
