package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a , b; 
		a = sc.nextInt();
		b = sc.nextInt();
		
		for(int i = 0; i<b; i++) {
			System.out.println(a + b);
		}
		for(int i = 0; i<b; i++) {
			System.out.println(a + b);
		}
		for(int i = 0; i<b; i++) {
			System.out.println(a + b);
		}
		for(int i = 0; i<b; i++) {
			System.out.println(a + b);
		}
		for(int i = 0; i<b; i++) {
			System.out.println(a + b);
		}
		
		
	}
	

}
