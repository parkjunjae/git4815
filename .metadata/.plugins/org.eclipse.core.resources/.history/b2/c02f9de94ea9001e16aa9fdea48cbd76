package ex02;

import java.util.Scanner;

public class Ex08 {
	public static void main(String[] args) {
		//nextLine 문자열 입력
		Scanner scan = new Scanner(System.in);
		
		System.out.println("숫자입력");
		
		int num1 = scan.nextInt();
	
		System.out.println("숫자입력");
		
		int num2 = scan.nextInt();
		
		System.out.println("결과 = "+(num1 + num2)
				);
	}
}
