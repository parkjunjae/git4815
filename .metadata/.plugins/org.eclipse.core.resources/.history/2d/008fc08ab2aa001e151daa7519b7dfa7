package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n, m ;
		n = sc.nextInt();
		m = sc.nextInt();
		int[] answer = new int[n];		
		for(int i =0; i< m ; i++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			int c = sc.nextInt();
		for(int j =a - 1; j< b; j++) {
			answer[j] = c;
			}
		}
		for(int a =0; a < n; a++) {
			System.out.println(answer[a] + " ");
		}
		
		
	}

}