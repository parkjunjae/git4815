package ex03;

import java.util.Scanner;

public class Ex06 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt();

		while (true) {
			if (a == 0 && b == 0) {
				sc.close();
			}
			break;
		}
		System.out.println(a + b);
	}

}
